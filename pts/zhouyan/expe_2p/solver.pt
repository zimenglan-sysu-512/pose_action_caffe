# net pt
net: "pts/zhouyan/expe_2p/train.pt"

# snapshot
snapshot: 500

# snapshot prefix
snapshot_prefix: "../asserts/pts/zhouyan/expe_2p/models/expe_2p"

# 504 = 126 * 4
test_iter: 126
test_interval: 1000
test_initialization: true

# 0.00001 fast convergence
base_lr: 0.00001 

# Learning rate
lr_policy: "step"

# SGD
gamma: 0.0001
power: 0.75
momentum: 0.9
weight_decay: 0.0005

# Change learning rate
stepsize: 16000

# Iterations
max_iter: 16000

# Display
display: 20

# CPU or GPU
solver_mode: GPU

# GPU ID
device_id: 0
