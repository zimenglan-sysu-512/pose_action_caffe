  name: "conv4_2_bank1"
  type: "Convolution"
  bottom: "conv4_1_bank1"
  top: "conv4_2_bank1"
  param {
    lr_mult: 0
    decay_mult: 0
  }
  param {
    lr_mult: 0
    decay_mult: 0
  }
  convolution_param {
    num_output: 512
    pad: 4
    kernel_size: 9
    weight_filler {
      type: "gaussian"
      std: 0.01
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}

layer {
  name: "relu4_2_bank1"
  type: "ReLU"
  bottom: "conv4_2_bank1"
  top: "conv4_2_bank1"
}

# ##############################
# bank2
# conv & relu & pool & resize
