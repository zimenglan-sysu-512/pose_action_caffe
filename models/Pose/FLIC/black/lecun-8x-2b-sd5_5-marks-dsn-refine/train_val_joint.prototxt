  top: "crop_conv4_bank1"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  convolution_param {
    num_output: 3584  # 256
    pad: 4
    kernel_size: 9
    group: 14
    weight_filler {
      type: "gaussian"
      std: 0.01
    }
    bias_filler {
      type: "constant"
      value: 0.
    }
  }
}
layer {
  name: "crop_relu4_bank1"
  type: "ReLU"
