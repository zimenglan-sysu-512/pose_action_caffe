    }
  }
}

layer {
  name: "relu4_2_bank2"
  type: "ReLU"
  bottom: "conv4_2_bank2"
  top: "conv4_2_bank2"
}

layer {
  name: "re_conv4_2_bank2"
  type: "Resize"
  bottom: "conv4_2_bank2"
  bottom: "conv4_2_bank1"
  top: "re_conv4_2_bank2"
}

# ##############################
# merge
# conv & relu & pool & resize
layer {
  name: "merge_banks"
  type: "Eltwise"
  bottom: "conv4_2_bank1"
  bottom: "re_conv4_2_bank2"
  top: "conv4"
  eltwise_param {
    operation: SUM
  }
}

layer {
  name: "spatial_drop"
  type: "SpatialDropout"
  bottom: "conv4"
  top: "conv4"
  spatial_dropout_param {
    dropout_ratio: 0.5
  }
}

# ########### conv5
layer {
  name: "conv5"
  type: "Convolution"
  bottom: "conv4"
  top: "conv5"
  param {
    lr_mult: 0
