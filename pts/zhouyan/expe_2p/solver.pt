# net pt
net: "pts/zhouyan/expe_2p/train.pt"

# snapshot
snapshot: 500

# snapshot prefix
snapshot_prefix: "../asserts/pts/zhouyan/expe_2p/models/expe_2p"

test_iter: 1
test_interval: 1000000
test_initialization: false

# 0.0001 fast convergence
base_lr: 0.0001 

# Learning rate
lr_policy: "step"

# SGD
gamma: 0.0001
power: 0.75
momentum: 0.9
weight_decay: 0.0005

# Change learning rate
stepsize: 8000

# Iterations
max_iter: 8000

# Display
display: 20

# CPU or GPU
solver_mode: GPU

# GPU ID
device_id: 0
