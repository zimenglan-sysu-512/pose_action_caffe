    decay_mult: 0
  }
  convolution_param {
    num_output: 64
    kernel_size: 7
    pad: 3
    weight_filler {
      type: "gaussian"
      std: 0.01
    }
    bias_filler {
      type: "constant"
    }
  }
}

layer {
  name: "relu1_f"
  type: "ReLU"
  bottom: "conv1_f"
  top: "conv1_f"
}

# ########### conv2_f
layer {
  name: "conv2_f"
  type: "Convolution"
  bottom: "conv1_f"
  top: "conv2_f"
  param {
