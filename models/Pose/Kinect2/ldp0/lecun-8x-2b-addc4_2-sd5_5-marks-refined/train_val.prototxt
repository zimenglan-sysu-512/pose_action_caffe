layer {
  name: "accuracy"
  type: "PosePDJAccuracy"
  bottom: "pred_pose_coords"
  bottom: "gt_origin_pose_coords"
  top: "accuracy"
  include {
    phase: TEST
  }
  pose_pdj_accuracy_param {
    acc_factor: 0.05
    acc_factor_num: 10
    images_num: 13707
    acc_name: "pose_pdj_acc.log"
    log_name: "pose_pdj_log.log"
    acc_path: "../asserts/models/Pose/Kinect2/ldp0/lecun-8x-2b-addc4_2-sd5_5-marks-refined/test_acc/"
    # (rsho: 5,lhip: 16) 
    # index starts from zero
    shoulder_id: 5
    hip_id: 16
  }
