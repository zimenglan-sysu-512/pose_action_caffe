layer {
  name: "accuracy"
  type: "PosePDJAccuracy"
  bottom: "pred_coordsr"
  bottom: "gt_coordsr"
  top: "accuracy"
  include {
    phase: TEST
  }
  pose_pdj_accuracy_param {
    acc_factor: 0.05
    acc_factor_num: 10
    images_num: 13707
    acc_name: "accuracy.log"
    log_name: "log.log"
    acc_path: "../asserts/models/Pose2/kinect2_motion/final/fusnet_tmask_2b_1/accuracy/"
    # here use (rsho, lhip) -> 5: rsho, 16: lhip
    shoulder_id: 5
    hip_id: 16
    zero_iter_test: true
  }
}

layer {
  name: "accuracy_eudist"
  type: "PoseEuDistAccuracy"
  bottom: "pred_coordsr"
  bottom: "gt_coordsr"
  top: "accuracy_eudist"
  include {
    phase: TEST
  }
  pose_eudist_accuracy_param {
    acc_factor: 1
    acc_factor_num: 24
    images_num: 13707
    acc_name: "accuracy2.log"
    log_name: "log2.log"
    acc_path: "../asserts/models/Pose2/kinect2_motion/final/fusnet_tmask_2b_1/accuracy/"
    zero_iter_test: true
  }
}

