# ########### loss 2
layer {
  name: "loss2"
  type: "PoseHeatMapLoss"
  bottom: "conv5_f"
  bottom: "gt_conv8"
  bottom: "heat_maps_masks"
  top: "loss2"
  include {
    phase: TRAIN
  }
  loss_weight: 3
  pose_heat_map_loss_param {
    loss_emphase_type: 3
    fg_eof: 3
    bg_eof: 1
    ratio: 0.005
    prob_num: 11
    heat_score_thres: 0.273
    parts_err_num_thres: 3
    hard_negative_filepath: "NotImplemented."
  }
}

# ########### conv9
layer {
  name: "gt_coordsr"
  type: "RescaledPoseCoords"
  bottom: "gt_coords"
  bottom: "aux_info"
  top: "gt_coordsr"
  include {
    phase: TEST
  }
}

layer {
  name: "conv9"
  type: "Eltwise"
  bottom: "conv8"
  bottom: "conv5_f"
  top: "conv9"
  eltwise_param {
    operation: SUM
    coeff: 1
    coeff: 1
  }
  include {
    phase: TEST
  }
}

layer {
  name: "pred_heat_maps"
  type: "Resize"
