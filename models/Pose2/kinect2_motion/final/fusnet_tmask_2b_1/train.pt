  name: "conv3_b2r"
  type: "Resize"
  bottom: "conv3_b2"
  bottom: "conv3_b1"
  top: "conv3_b2r"
}

layer {
  name: "concat_f"
  type: "Concat"
  bottom: "conv3_b1"
  bottom: "conv3_b2r"
  bottom: "conv7"
  top: "concat_f"
  concat_param {
    concat_dim: 1
  }
}

