  type: "Convolution"
  bottom: "crop_conv1_bank1"
  top: "crop_conv2_bank1"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  convolution_param {
    num_output: 1216
    pad: 2
    group: 19
    kernel_size: 5
    weight_filler {
      type: "gaussian"
      std: 0.01
    }
